<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.spark.chipro.manage.biz.mapper.TaskMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.spark.chipro.manage.biz.entity.Task">
        <id column="TASK_ID" property="taskId" />
        <result column="NAME" property="name" />
        <result column="ASSIGNED_CLASS" property="assignedClass" />
        <result column="TITLE" property="title" />
        <result column="WORKS_COUNT" property="worksCount" />
        <result column="WORKS_REQUIRE" property="worksRequire" />
        <result column="EDITOR" property="editor" />
        <result column="RESOURCE_ID" property="resourceId" />
        <result column="DEADLINE" property="deadline" />
        <result column="CREATE_USER" property="createUser" />
        <result column="CREATE_TIME" property="createTime" />
        <result column="UPDATE_USER" property="updateUser" />
        <result column="UPDATE_TIME" property="updateTime" />
        <result column="REMARKS" property="remarks" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        TASK_ID AS "taskId", NAME AS "name", ASSIGNED_CLASS AS "assignedClass", TITLE AS "title", WORKS_COUNT AS "worksCount", WORKS_REQUIRE AS "worksRequire", EDITOR AS "editor", RESOURCE_ID AS "resourceId", DEADLINE AS "deadline", CREATE_USER AS "createUser", CREATE_TIME AS "createTime", UPDATE_USER AS "updateUser", UPDATE_TIME AS "updateTime", REMARKS AS "remarks"
    </sql>

</mapper>
